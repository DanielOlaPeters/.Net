@model Lab3.Models.Student

@{
     Layout = "_Layout";
    ViewData["Title"] = "Display Student data";
}
<h3>Your Favorite Student</h3>
<table>
        <tr>
        <td>
            First Name:
        </td>
        <td>
            @Model.firstName
        </td>
    </tr>
        <tr>
        <td>
            Last Name:
        </td>
        <td>
            @Model.lastName
        </td>
    </tr>
        <tr>
        <td>
            StudentId:
        </td>
        <td>
            @Model.studentId
        </td>
    </tr>
    <tr>
        <td>
            Email Address:
        </td>
        <td>
            @Model.EmailAddress
        </td>
    </tr>
    <tr>
        <td>
            Description:
        </td>
        <td>
            @Model.description
        </td>
    </tr>
</table>
<br />
